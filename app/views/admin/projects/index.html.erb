<div class="page-header">
  <h1>Projects</h1>
</div>
<table class="table table-bordered table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th>Body</th>
      <th>Name</th>
      <th>Description</th>
      <th>URL</th>
      <th>Picture</th>
      <th>Name1</th>
      <th>Description1</th>
      <th>URL1</th>
      <th>Picture1</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= simple_format project.body %></td>
        <td><%= project.name %></td>
        <td><%= simple_format project.description %></td>
        <td><%= project.url %></td>
        <td><%= project.picture %></td>
        <td><%= project.name1 %></td>
        <td><%= simple_format project.description1 %></td>
        <td><%= project.url1 %></td>
        <td><%= project.picture1 %></td>
        <td><%=l project.created_at %></td>
        <td><%=l project.updated_at %></td>
        <td>
          <%= link_to admin_project_path(project), :class => "btn btn-info" do %>
            R
          <% end %>
          <%= link_to edit_admin_project_path(project), :class => "btn btn-success" do %>
            U
          <% end %>
          <%= link_to admin_project_path(project), method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" do %>
            D
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<div style="clear:both;"></div>

<%= link_to "New",
            new_admin_project_path,
            :class => 'btn btn-primary' %>